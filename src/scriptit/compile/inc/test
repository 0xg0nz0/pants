# simple incremental compile test

zinc -nailed -debug src/A.scala src/B.scala

exists A.class B.class

copy changes/A1.scala src/A.scala

! zinc -nailed -debug src/A.scala src/B.scala

copy changes/B1.scala src/B.scala

zinc -nailed -debug src/A.scala src/B.scala

exists A.class B.class

zinc -shutdown
